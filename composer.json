{
    "name": "symfony/symfony-demo",
    "license": "MIT",
    "type": "project",
    "description": "Symfony Demo Application",
    "require": {
        "php": "^7.1.3",
        "ext-pdo_sqlite": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "enqueue/amqp-ext": "^0.8.11",
        "erusev/parsedown": "^1.6",
        "ezyang/htmlpurifier": "^4.9",
        "guzzlehttp/guzzle": "~6.0",
        "sensio/framework-extra-bundle": "^5.0",
        "sensiolabs/security-checker": "^4.1",
        "sroze/enqueue-bridge": "dev-master",
        "symfony/flex": "^1.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/symfony": "dev-add-message-component as 4.0",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.4",
        "white-october/pagerfanta-bundle": "^1.1"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^4.0",
        "friendsofphp/php-cs-fixer": "^2.7",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/debug-bundle": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "^4.0"
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sroze/message"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sroze/symfony"
        },
        {
            "type": "vcs",
            "url": "https://github.com/sroze/framework-bundle"
        }
    ],
    "extra": {
        "symfony": {
            "id": "01BY8QJHBGTYRP06WQFNY99EKP",
            "allow-contrib": true
        }
    }
}
